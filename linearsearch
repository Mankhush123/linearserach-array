//linear search
class ArraysExamples{
    void searchInArrays(){
        int arr[]={3,4,5,6,8,7,9};
        int x=4;
        int index=-1;
        for(int i=0;i<arr.length;i++){
        if(arr[i]==x){
        index=i;
        }
        }
        System.out.println("found"+x+"at index"+index);


        }
        }









public class Main{
    public static void main(String args[]){
    ArraysExamples obj=new ArraysExamples();
    obj.searchInArrays();
    }
}
